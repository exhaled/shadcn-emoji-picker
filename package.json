{
	"name": "shadcn-emoji-picker",
	"version": "1.0.0",
	"description": "The emoji picker component created with Shadcn UI, fork of Emoogle Emoji Picker",
	"keywords": [
		"emoji",
		"emoji picker",
		"React",
		"Emoogle",
		"Shadcn"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xitanggg/emoogle-emoji-picker.git"
	},
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.js",
	"files": [
		"dist"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js"
		},
		"./dist/index.css": {
			"import": "./dist/index.css"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "vite",
		"format": "prettier --write .",
		"check-format": "prettier --check .",
		"lint": "tsc",
		"ci": "npm run build && npm run check-format && npm run lint",
		"local-release": "changeset version && changeset publish",
		"prepublishOnly": "npm run ci",
		"create-data": "tsx scripts/create-data"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.9",
		"@types/node": "^22.10.1",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.3",
		"autoprefixer": "^10.4.20",
		"emoogle-emoji-search-engine": "^1.0.0",
		"json-stringify-pretty-compact": "^4.0.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.14",
		"tsup": "^8.3.5",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
		"vite": "^5.4.10",
		"zustand": "^5.0.1"
	},
	"peerDependencies": {
		"emoogle-emoji-search-engine": ">=1",
		"react": ">=17",
		"react-dom": ">=17",
		"zustand": ">=4"
	},
	"dependencies": {
		"@radix-ui/react-dropdown-menu": "^2.1.2",
		"@radix-ui/react-scroll-area": "^1.2.2",
		"@radix-ui/react-slot": "^1.1.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.464.0",
		"tailwind-merge": "^2.5.5",
		"tailwindcss-animate": "^1.0.7",
		"usehooks-ts": "^3.1.0"
	}
}
